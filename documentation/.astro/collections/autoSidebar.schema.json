{
  "$ref": "#/definitions/autoSidebar",
  "definitions": {
    "autoSidebar": {
      "type": "object",
      "properties": {
        "badge": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "object",
              "properties": {
                "class": {
                  "type": "string"
                },
                "text": {
                  "type": "string"
                },
                "variant": {
                  "type": "string",
                  "enum": [
                    "note",
                    "danger",
                    "success",
                    "caution",
                    "tip",
                    "default"
                  ],
                  "default": "default"
                }
              },
              "required": [
                "text"
              ],
              "additionalProperties": false
            }
          ]
        },
        "cascade": {
          "type": "array",
          "items": {
            "type": "string",
            "enum": [
              "collapsed",
              "sort"
            ]
          }
        },
        "collapsed": {
          "type": "boolean",
          "default": false
        },
        "depth": {
          "type": "number",
          "minimum": 1
        },
        "hidden": {
          "type": "boolean",
          "default": false
        },
        "label": {
          "type": "string"
        },
        "order": {
          "type": "number"
        },
        "sort": {
          "type": "string",
          "enum": [
            "slug",
            "reverse-slug"
          ],
          "default": "slug"
        },
        "$schema": {
          "type": "string"
        }
      },
      "additionalProperties": false
    }
  },
  "$schema": "http://json-schema.org/draft-07/schema#"
}