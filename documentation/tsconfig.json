{
  "extends": "../tsconfig.base.json",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "preact",
    "lib": ["ES2023", "dom", "dom.iterable"],
    "skipLibCheck": true,
    "paths": {
      "@/styles/*": [
        "./src/styles/*"
      ],
      "@/fonts/*": [
        "./src/fonts/*"
      ],
      "@/assets/*": [
        "./src/assets/*"
      ],
      "@/components/*": [
        "./src/components/*"
      ],
      "@/features/*": [
        "./src/features/*"
      ],
      "@/layouts/*": [
        "./src/layouts/*"
      ],
      "@/pages/*": [
        "./src/pages/*"
      ],
      "@/utils/*": [
        "./src/utils/*"
      ],
      "@/sections/*": [
        "./src/sections/*"
      ],
      "@/api/*": [
        "./src/api/*"
      ],
      "@/content/*": [
        "./src/content/*"
      ],
      "@/types/*": [
        "./src/types/*"
      ],
      "@/overrides/*": [
        "./src/overrides/*"
      ],
      "@/utils/configuration/*": [
        "./src/utils/configuration/*"
      ],
    }
  }
}
